    "    df_taste = spark.read.parquet(taste_path)\n",
    "    count = df_taste.count()\n",
    "    row_counts['tasteprofile'] = count\n",
    "    print(f\"  • tasteprofile: {count:,} rows\")\n",
    "except Exception as e:\n",
